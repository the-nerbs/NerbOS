<Project ToolsVersion="15.0">
  <PropertyGroup>
    <TargetName Condition="'$(TargetName)'==''">$(MSBuildProjectName)</TargetName>
    <TargetExtension Condition="'$(TargetExtension)'==''">.bin</TargetExtension>

    <OutputPath Condition="'$(OutputPath)'==''">$(BinDir)</OutputPath>
    <TargetDir Condition="'$(TargetDir)'==''">$(OutputPath)</TargetDir>

    <TargetPath Condition="'$(TargetPath)'==''">$(TargetDir)$(TargetName)$(TargetExtension)</TargetPath>
  </PropertyGroup>

  <Import Project="$(MSBuildThisFileDirectory)common.props" />

  <PropertyGroup>
    <CompileDependsOn>
      FASMAssemble;
      $(CompileDependsOn)
    </CompileDependsOn>
  </PropertyGroup>
</Project>
